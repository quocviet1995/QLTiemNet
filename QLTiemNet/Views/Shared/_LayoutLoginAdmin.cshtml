<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/angular.js"></script>
    <script src="~/HandleScripts/Service/Angular.Service.js"></script>
    <script src="~/HandleScripts/Service/LoginAdmin.service.js"></script>
    <script src="~/HandleScripts/LoginAdmin.controller.js"></script>
    <script src="~/Scripts/sweetalert2.all.js"></script>
    <img src="~/Asserts/logo.png" width="290" height="150" style="display:block; margin:0 auto" />
</head>
<body ng-app="myApp" style="background-color:#87cefa">
    <div class="container">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
